//Program to Print Prime Numbers Uptill N//
#include <iostream>

using namespace std;

int main()
{
int N, num=2;
cout<<"Enter an Integer:";
cin>>N;
while(num<N)
{
    int div=2;
    while(div<num)
    {
        if(num%div==0)
        num=num+1;
        else
        div=div+1;
    }
    cout<<num<<endl;
    num=num+1;
}
}
